      <option class="pickDest" value="INTO">INTO Building</option>
      <option class="pickDest" value="SAINSBURY">Sainsbury Centre</option>
    </select>
  </div>
  <div>
    <p class="dropbtn" for="arriveDate">Arrival Date:</p>
        <input type="date" id="arriveDate" name="arrival-start" max="2032-12-31">
  </div>
  <!-- set minimum date to current day -->
  <script>
    document.getElementById('arriveDate').value = new Date().toISOString().substring(0, 10);
    document.getElementById('arriveDate').min = new Date().toISOString().substring(0, 10);
  </script>
</div>
