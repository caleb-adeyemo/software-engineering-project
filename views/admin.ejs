    <label class="dropbtn" for="myDropdown">Destination</label>
    <select id="myDropdown" name="destination">
      <option class="pickDest" value="LIBRARY">Library</option>
      <option class="pickDest" value="SU">Student Union</option>
      <option class="pickDest" value="VILLAGE">University Village</option>
      <option class="pickDest" value="SPORTSPARK">Sportspark</option>
